{
    "manifest_version": 3,
    "name": "Adblockx",
    "version": "1.0",
    "description": "first attempt at an adblocker + using git",
    "permissions": ["declarativeNetRequest", "declarativeNetRequestFeedback"],
    "host_permissions": ["<all_urls>"],
    "background": {
        "service_worker": "b.js"
    },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
    "icons": {
        "16": "icons/i.png",
        "48": "icons/i.png",
        "128": "icons/i.png"
    }
}